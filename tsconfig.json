{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "outDir": "./admin/static/js",
    "rootDir": "./admin/static/js"
  },
  "include": ["./admin/static/js/**/*.ts"]
}
